    function nFormatter(num, digits)
    {
	      const lookup = [
	        { value: 1, symbol: "" },
	        { value: 1e3, symbol: "k" },
	        { value: 1e6, symbol: "M" },
	        { value: 1e9, symbol: "G" },
	        { value: 1e12, symbol: "T" },
	        { value: 1e15, symbol: "P" },
	        { value: 1e18, symbol: "E" }
	      ];
	      if(num<1)
	      {
	        return (Math.round(num*1000)/1000);
	      }
	  const rx = /\.0+$|(\.[0-9]*[1-9])0+$/;
	  var item = lookup.slice().reverse().find(function(item) {
	    return num >= item.value;
	  });
	  return item ? (num / item.value).toFixed(digits).replace(rx, "$1") + item.symbol : "0";
	}

	function nReverseFormatter(num)
      {
		let token_balance_formatted = num;
		if(token_balance_formatted.includes("k"))
		{
			token_balance_formatted = Number(token_balance_formatted.slice(0, -1));
			return token_balance_formatted*(10**3);
		}
		else if(token_balance_formatted.includes("M"))
		{
			token_balance_formatted = Number(token_balance_formatted.slice(0, -2));
			return token_balance_formatted*(10**6);
		}
		else if(token_balance_formatted.includes("G"))
		{
			token_balance_formatted = Number(token_balance_formatted.slice(0, -2));
			return token_balance_formatted*(10**9);
		}
		else if(token_balance_formatted.includes("T"))
		{
			token_balance_formatted = Number(token_balance_formatted.slice(0, -2));
			return token_balance_formatted*(10**12);
		}
		else if(token_balance_formatted.includes("P"))
		{
			token_balance_formatted = Number(token_balance_formatted.slice(0, -2));
			return token_balance_formatted*(10**15);
		}
		else if(token_balance_formatted.includes("E"))
		{
			token_balance_formatted = Number(token_balance_formatted.slice(0, -2));
			return token_balance_formatted*(10**18);
		}
  	  return num;
  	}
